<br><br>
<%= @store.name %>
<br><br>
<% @transactions.each do |transaction| %>
  <% transaction_type = transaction.transaction_type %>
  <%= transaction_type.signal %><br>
  <%= transaction_type.nature %><br>
  <%= transaction_type.description %><br>
  <%= display_currency(transaction.value) %><br>
  <%= transaction.card %><br>
  <%= transaction.cpf %><br>
  <%= "Data e Hora: #{transaction.date_and_time.strftime('%d/%m/%Y %H:%M:%S')}" %><br>
  <br><br>
<% end %>